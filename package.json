{
  "name": "eyedropper.app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Pick colors from the screen in Chromium browsers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanbabcock/eyedropper.app.git"
  },
  "scripts": {
    "start": "vite --open",
    "dev": "vite --open",
    "sync": "env-cmd -f .env aws s3 sync dist s3://eyedropper.app",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "clsx": "^2.1.0",
    "react": "^19.0.0-canary",
    "react-dom": "^19.0.0-canary",
    "react-icons": "^5.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.2.5",
    "typescript": "5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-svgr": "^4.2.0"
  }
}
