{
  "name": "eyedropper.app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Pick colors from the screen in Chromium browsers.",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanbabcock/eyedropper.app.git"
  },
  "scripts": {
    "start": "vite --open",
    "dev": "vite --open",
    "sync": "env-cmd -f .env aws s3 sync dist s3://eyedropper.app",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "clsx": "^2.1.0",
    "react": "^19.0.0-canary",
    "react-dom": "^19.0.0-canary",
    "react-icons": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.5.0-dev",
    "vite": "^5.2.10",
    "vite-plugin-svgr": "^4.2.0"
  }
}
